    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.26.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended": "^3.0.0",
